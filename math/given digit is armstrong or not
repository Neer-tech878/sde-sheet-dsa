// User function Template for Java
class Solution {
    static boolean armstrongNumber(int n) {
         int original = n;
         int ans = 0;
        while(n!=0){
        int digit=n%10;
        ans = ans + (digit*digit*digit);
        n/=10;
       }
       return ans==original;
    }
}
//checks the digit is armstrong or not
