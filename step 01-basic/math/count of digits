import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter n:");
        int n = sc.nextInt();
        if(n<=0){
            System.out.println("0");
        }
        int count =0;
        while(n>0){
            int lastdigit = n%10;
            System.out.println(lastdigit);
            n=n/10;
            count++;
        }
        System.out.print("count:"+count);
    }
}
